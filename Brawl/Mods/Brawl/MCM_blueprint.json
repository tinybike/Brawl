                        }, {
                            "Id": "full_auto",
                            "Name": "Fully Automated Brawls",
                            "Type": "checkbox",
                            "Default": false,
                            "Description": "If checked, the companion AI will control ALL player characters, including the active character.",
                            "Handles": {"DescriptionHandle": "h0c4f4c12g54a4g4d43g9dd9g0f61857119d9", "NameHandle": "hd6431cb2g76b4g497bgaccbg202b25c375c7"}
                        }, {
                            "Id": "hitpoints_multiplier",
                            "Name": "Hitpoints Multiplier",
                            "Type": "slider_float",
                            "Default": 1.0,
                            "Options": {"Min": 0.1, "Max": 10.0},
                            "Description": "Increase or decrease the health of all units (players and enemies) by this factor, e.g. a value of 3 will triple all health values.",
                            "Handles": {"DescriptionHandle": "hc26cac3bg1de2g4808g852fga815daf697bd", "NameHandle": "hd2d7dc1ag806eg4339gb306g679658313ab2"}
                        }, {
                            "Id": "active_character_archetype",
                            "Name": "Active Character Archetype",
                            "Type": "enum",
                            "Default": "",
                            "Options": {"Choices": ["", "melee", "mage", "ranged", "healer", "healer_melee", "melee_magic"]},
                            "Description": "Combat archetype for your currently selected character",
                            "Handles": {"DescriptionHandle": "h5fba396bg3ea9g42c8g99f7gd52fad9d7c46", "NameHandle": "h08b6adc8ge129g45a0ga62bgdd4a013d0e2b"}
                        }, {
                            "Id": "companion_tactics",
                            "Name": "Companion Tactics",
                            "Type": "enum",
                            "Default": "Offense",
                            "Options": {"Choices": ["Offense", "Defense"]},
                            "Description": "High-level behavior for the companion AI.  Offense: all-out attack, prioritize killing enemies.  Defense: prioritize staying with the group, don't run off alone to attack.",
