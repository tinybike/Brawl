                    ]
                }, {
                    "SectionId": "features",
                    "SectionName": "Features (Host)",
                    "Settings": [
                        {
                            "Id": "companion_ai_enabled",
                            "Name": "Enable Companion AI",
                            "Type": "checkbox",
                            "Default": true,
                            "Description": "Toggle the companion AI on or off",
                            "Handles": {"DescriptionHandle": "h610d142agadb3g4d87ga5deg186ff031831c", "NameHandle": "hae8f7216g6047g46adg839bga877ccbd6147"}
                        }, {
                            "Id": "true_pause",
                            "Name": "True Pause",
                            "Type": "checkbox",
                            "Default": true,
                            "Description": "If checked, your characters will queue up actions during pause, and fire them when unpaused, similar to how combat worked in Baldur's Gate 1 and 2.",
                            "Handles": {"DescriptionHandle": "h65ee56d8gbbbeg4e6agb486ge33349a73d09", "NameHandle": "h7a39e407g21e0g4f3bg98fdg7cd473b01043"}
                        }, {
                            "Id": "auto_pause_on_downed",
                            "Name": "Automatic Pause On Downed",
                            "Type": "checkbox",
                            "Default": true,
                            "Description": "If checked, the game will automatically pause when the character the player is controlling goes down",
                            "Handles": {"DescriptionHandle": "h610d142agadb3g4d87ga5deg186ff031831c", "NameHandle": "hae8f7216g6047g46adg839bga877ccbd6147"}
                        }, {
                            "Id": "action_interval",
                            "Name": "AI Turn Length",
                            "Type": "int",
                            "Default": 6000,
                            "Description": "The AI's turn length, in milliseconds.  The default is 6000, meaning the AI will act once every 6 seconds.  Higher values make combat easier, lower values make combat harder.",
